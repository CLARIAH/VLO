  </facetConcept>
  <facetConcept name="collection">
    <pattern>/c:CMD/c:Header/c:MdCollectionDisplayName/text()</pattern>
  </facetConcept>
  <facetConcept name="projectName">
    <concept>http://www.isocat.org/datcat/DC-2536</concept>
